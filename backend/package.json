    "main": "index.js",
    "license": "MIT",
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.3.6",
        "@types/aws-sdk": "^2.7.0",
        "@types/cors": "^2.8.11",
        "@types/express": "^4.17.12",
        "@types/jest": "^26.0.23",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/node": "^16.0.0",
        "@types/nodemailer": "^6.4.4",
        "@types/pg": "^8.6.0",
        "@types/sharp": "^0.29.2",
        "@types/uuid": "^8.3.1",
        "jest": "^27.0.6",
        "jest-junit": "^12.2.0",
        "ts-jest": "^27.0.3",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5"
    },
    "scripts": {
        "start-dev": "ts-node-dev ./index.ts",
        "start": "ts-node ./index.ts",
        "postinstall": "tsc",
        "genclient": "openapi-generator-cli generate --generator-name typescript-axios --input-spec ./openapi.yaml --output ./src/client",
        "test": "jest --force-exit --detectOpenHandles",
        "test_vast": "ts-node ./src/test_vast.ts",
        "test_ec2": "ts-node ./src/test_ec2_engine.ts",
        "test_runpod": "ts-node ./src/test_runpod.ts",
        "build_frontend": "rm -rf ./public && cd ../frontend && yarn build && cp -r build ../backend/public"
    },
    "dependencies": {
        "aws-sdk": "^2.1066.0",
        "axios": "^0.22.0",
        "cors": "^2.8.5",
        "dotenv": "^14.3.2",
        "express": "^4.17.1",
        "express-openapi-validator": "^4.12.14",
        "http-terminator": "^3.0.0",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "nodemailer": "^6.6.5",
        "pg": "^8.6.0",
        "postgres-migrations": "^5.2.0",
        "sharp": "^0.29.1",
        "ts-node": "^10.0.0",
        "uuid": "^8.3.2",
        "yarn": "^1.22.10"
    },
    "engines": {
        "node": ">=14.x"
    }
